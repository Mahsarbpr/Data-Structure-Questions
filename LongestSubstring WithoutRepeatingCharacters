 public int LengthOfLongestSubstring(string s) {
        
        if(s == "") return 0;
        
        var sLength = s.Length;
        var maxLength=0;
        for(var i = 0; i< sLength; i++)
        {
            var charDict = new HashSet<char>();
            for(var j = i; j < sLength; j++)
            {
                if(charDict.Contains(s[j]))
                {
                    var localMax = charDict.Count;
                    maxLength = Math.Max(maxLength, localMax);
                    break;
                }
                else {
                    charDict.Add(s[j]);
                    };
            }
            maxLength =Math.Max(maxLength, charDict.Count);
        }
        return maxLength;
    }
